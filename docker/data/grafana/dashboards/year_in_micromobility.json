          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH filtered AS (\r\n  SELECT\r\n    day::date AS day,\r\n    network_name_lend,\r\n    SUM(trip_count) AS daily_total\r\n  FROM daily_trips_agg\r\n  WHERE EXTRACT(YEAR FROM day)::int = $year\r\n  GROUP BY day::date, network_name_lend\r\n)\r\nSELECT\r\n  day::timestamp AS time,\r\n  RIGHT(network_name_lend::text, 2) AS network_suffix,\r\n  SUM(daily_total) OVER (\r\n    PARTITION BY RIGHT(network_name_lend::text, 2)\r\n    ORDER BY day\r\n    ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW\r\n  ) AS \"Cumulative Trips\"\r\nFROM filtered\r\nORDER BY time, network_name_lend;\r\n",
          "refId": "A",
