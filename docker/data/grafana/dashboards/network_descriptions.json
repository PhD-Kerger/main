          "rawSql": "SELECT\r\n  duration_minutes,\r\n  network_name_lend::text AS network_suffix\r\nFROM (\r\n  SELECT\r\n    FLOOR(EXTRACT(EPOCH FROM AGE(timestamp_return, timestamp_lend)) / 60) AS duration_minutes,\r\n    network_name_lend\r\n  FROM trips\r\n  WHERE timestamp_return IS NOT NULL\r\n    AND timestamp_lend IS NOT NULL\r\n) AS subquery\r\nWHERE duration_minutes < 100 AND network_name_lend::text IN ($cities)\r\nLIMIT 10000;\r\n",
